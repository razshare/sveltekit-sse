{
  "name": "sveltekit-sse",
  "version": "0.14.3",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && bun run prepack",
    "preview": "vite preview",
    "prepare": "bunx svelte-kit sync || echo ''",
    "prepack": "bunx svelte-kit sync && bunx svelte-package && bunx publint",
    "prepublishOnly": "bun run check && bun run test && bun run build",
    "check": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "bunx svelte-kit sync && bunx svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "playwright test"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0 || ^5.0.0-next.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@playwright/test": "^1.54.1",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.26.1",
    "@sveltejs/package": "^2.4.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@types/eslint": "^9.6.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "bun-types": "latest",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte": "^5.36.17",
    "svelte-check": "^4.3.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "keywords": [
    "svelte"
  ]
}
